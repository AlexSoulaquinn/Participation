<Window x:Class="GOTQuotes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOTQuotes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://www.hbo.com/content/dam/hbodata/series/game-of-thrones/episodes/1/game-of-thrones-1-1920x1080.jpg/_jcr_content/renditions/cq5dam.web.1200.675.jpeg"/>
        </Grid.Background>
        <TextBox x:Name="txtQuote" HorizontalAlignment="Left" Height="63" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772"/>
        <Button Content="Get Quote" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top" Width="142" Height="35" Click="Button_Click"/>
        <TextBox x:Name="txtCharacter" HorizontalAlignment="Left" Height="23" Margin="596,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>

    </Grid>
</Window>
